{
    "window.commandCenter": 1,
    "files.autoSave": "onFocusChange"
    "editor.formatOnType": true,
    "debug.disassemblyView.showSourceCode": false,
    "[vimrc]": {
      "editor.formatOnSave": false
    },
    "vim.showMarksInGutter": true,
    "vim.leader": "<space>",
    "vim.statusBarColorControl": true,
    "editor.stickyScroll.enabled": true,
    "vim.visualModeKeyBindingsNonRecursive": [
      { "before": ["L"], "after": ["$"] },
      { "before": ["H"], "after": ["^"] },
      { "before": ["J"], "after": ["}"] },
      { "before": ["K"], "after": ["{"] },
      { "before": ["U"], "after": ["<c-r>"] },
      { "before": ["t"], "after": ["."] },
      { "before": [","], "commands": ["workbench.action.previousEditor"] },
      { "before": ["."], "commands": ["woÏ€rkbench.action.nextEditor"] },
      { "before": ["]"], "after": [";"] },
      { "before": ["["], "after": [","] },
      { "before": ["}"], "commands": [ "editor.action.smartSelect.expand" ] },
      { "before": ["{"], "commands": [ "editor.action.smartSelect.shrink" ] },
      { "before": ["'"], "after": ["j", "<c-e>", "j", "<c-e>", "j", "<c-e>", "j", "<c-e>", "j", "<c-e>"] },
      { "before": [";"], "after": ["k", "<c-y>", "k", "<c-y>", "k", "<c-y>", "k", "<c-y>", "k", "<c-y>"] },
      { "before": ["<leader>", "d"], "after": ["/", "^", "c", "l", "a", "s", ".", "*", "D", "e", "b", "<CR>"] },
      { "before": ["<leader>", "l"], "commands": [ "workbench.action.gotoLine" ] },
      { "before": ["<leader>", "k"], "commands": [ "outline.focus" ] },
      { "before": ["g", "u"], "commands": [ "relatedFiles.openBuildFile" ] },
      { "before": ["z", "l"], "commands": [ "fixdeps.fixCurrentFile" ] },
      { "before": ["<leader>", "r"], "commands": [ "editor.debug.action.runToCursor" ] },
      { "before": ["<leader>", "k"], "commands": [ "workbench.action.gotoSymbol" ] },
      { "before": ["<leader>", "h"], "commands": [ "blame.toggleBlame" ] },
      { "before": ["<leader>", "."], "commands": [ "workbench.action.focusNextGroup" ] },
      { "before": ["<leader>", ","], "commands": [ "workbench.action.focusPreviousGroup" ] },
      { "before": ["<leader>", ">"], "commands": [ "workbench.action.moveEditorToRightGroup" ] },
      { "before": ["<leader>", "<"], "commands": [ "workbench.action.moveEditorToLeftGroup" ] },
      { "before": ["<leader>", "s"], "commands": [ "workbench.action.splitEditor" ] },
      { "before": ["<leader>", "c"], "commands": [ "workbench.files.action.compareWithClipboard" ] },
      { "before": ["<leader>", "j"], "commands": [ "workbench.action.quickOpen" ] },
      { "before": ["<leader>", "t"], "commands": ["copyGoogle3FilePath", "workbench.action.terminal.new"] },
    ],
    "vim.operatorPendingModeKeyBindings": [
      { "before": ["L"], "after": ["$"] },
      { "before": ["H"], "after": ["^"] },
      { "before": ["J"], "after": ["}"] },
      { "before": ["K"], "after": ["{"] },
      { "before": ["U"], "after": ["<c-r>"] },
      { "before": ["]"], "after": [";"] },
      { "before": ["["], "after": [","] },
      { "before": ["}"], "commands": [ "editor.action.smartSelect.expand" ] },
      { "before": ["{"], "commands": [ "editor.action.smartSelect.shrink" ] },
      { "before": ["'"], "after": ["j", "<c-e>", "j", "<c-e>", "j", "<c-e>", "j", "<c-e>", "j", "<c-e>"] },
      { "before": [";"], "after": ["k", "<c-y>", "k", "<c-y>", "k", "<c-y>", "k", "<c-y>", "k", "<c-y>"] },
      { "before": ["<leader>", "."], "commands": [ "workbench.action.focusNextGroup" ] },
      { "before": ["<leader>", ","], "commands": [ "workbench.action.focusPreviousGroup" ] },
      { "before": ["<leader>", ">"], "commands": [ "workbench.action.moveEditorToRightGroup" ] },
      { "before": ["<leader>", "<"], "commands": [ "workbench.action.moveEditorToLeftGroup" ] },
      { "before": ["<leader>", "s"], "commands": [ "workbench.action.splitEditor" ] },
      { "before": ["<leader>", "c"], "commands": [ "workbench.files.action.compareWithClipboard" ] },
      { "before": ["<leader>", "j"], "commands": [ "workbench.action.quickOpen" ] },
      { "before": ["<leader>", "t"], "commands": ["copyGoogle3FilePath", "workbench.action.terminal.new"] },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      { "before": ["L"], "after": ["$"] },
      { "before": ["H"], "after": ["^"] },
      { "before": ["J"], "after": ["}"] },
      { "before": ["K"], "after": ["{"] },
      { "before": ["U"], "after": ["<c-r>"] },
      { "before": ["t"], "after": ["."] },
      { "before": [","], "commands": ["workbench.action.previousEditor"] },
      { "before": ["."], "commands": ["workbench.action.nextEditor"] },
      { "before": ["]"], "after": [";"] },
      { "before": ["["], "after": [","] },
      { "before": ["}"], "commands": [ "editor.action.smartSelect.expand" ] },
      { "before": ["{"], "commands": [ "editor.action.smartSelect.shrink" ] },
      { "before": ["'"], "after": ["j", "<c-e>", "j", "<c-e>", "j", "<c-e>", "j", "<c-e>", "j", "<c-e>"] },
      { "before": [";"], "after": ["k", "<c-y>", "k", "<c-y>", "k", "<c-y>", "k", "<c-y>", "k", "<c-y>"] },
      { "before": ["<leader>", "d"], "after": ["/", "^", "c", "l", "a", "s", ".", "*", "D", "e", "b", "<CR>"] },
      { "before": ["<leader>", "l"], "commands": [ "workbench.action.gotoLine" ] },
      { "before": ["<leader>", "k"], "commands": [ "outline.focus" ] },
      { "before": ["g", "u"], "commands": [ "relatedFiles.openBuildFile" ] },
      { "before": ["z", "l"], "commands": [ "fixdeps.fixCurrentFile" ] },
      { "before": ["<leader>", "r"], "commands": [ "editor.debug.action.runToCursor" ] },
      { "before": ["<leader>", "k"], "commands": [ "workbench.action.gotoSymbol" ] },
      { "before": ["<leader>", "h"], "commands": [ "blame.toggleBlame" ] },
      { "before": ["<leader>", "."], "commands": [ "workbench.action.focusNextGroup" ] },
      { "before": ["<leader>", ","], "commands": [ "workbench.action.focusPreviousGroup" ] },
      { "before": ["<leader>", ">"], "commands": [ "workbench.action.moveEditorToRightGroup" ] },
      { "before": ["<leader>", "<"], "commands": [ "workbench.action.moveEditorToLeftGroup" ] },
      { "before": ["<leader>", "s"], "commands": [ "workbench.action.splitEditor" ] },
      { "before": ["<leader>", "c"], "commands": [ "workbench.files.action.compareWithClipboard" ] },
      { "before": ["<leader>", "j"], "commands": [ "workbench.action.quickOpen" ] },
      { "before": ["<leader>", "t"], "commands": ["copyGoogle3FilePath", "workbench.action.terminal.new"] },
  
    ],
    "workbench.editor.enablePreview": false,
    "terminal.integrated.accessibleViewFocusOnCommandExecution": true,
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.autoResume": "onLoad",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.tabs.showActiveTerminal": "always",
    "terminal.integrated.tabs.showActions": "never",
    "terminal.integrated.tabs.location": "left",
    "terminal.integrated.shellIntegration.history": 1000,
    "terminal.integrated.mouseWheelZoom": true,
    "terminal.integrated.confirmOnKill": "always",
    "terminal.integrated.confirmOnExit": "always",
    "terminal.integrated.commandsToSkipShell": [
      "workbench.action.togglePanel",
      "-workbench.action.terminal.kill",
      "workbench.action.terminal.focusTabs"
    ],
    "terminal.integrated.ignoreProcessNames": [],
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.persistentSessionScrollback": 10000,
    "terminal.integrated.scrollback": 10000,
    "editor.minimap.autohide": true,
    "editor.minimap.maxColumn": 100,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "workbench.colorTheme": "Dracula Soft",
    "workbench.colorCustomizations": {
      "editor.background": "#101012",
      "statusBar.background": "#005f87",
      "statusBar.noFolderBackground": "#005f87",
      "statusBar.debuggingBackground": "#005f87",
      "statusBar.foreground": "#ffffff",
      "statusBar.debuggingForeground": "#ffffff"
    },
    "terminal.integrated.fontSize": 13,
    "accessibility.signals.terminalCommandSucceeded": {
      "sound": "on"
    },
    "accessibility.signals.terminalCommandFailed": {
      "sound": "on"
    },
    "accessibility.signals.terminalQuickFix": {
      "sound": "on"
    }
}